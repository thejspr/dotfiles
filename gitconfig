[user]
  name = Jesper Kjeldgaard
  email = thejspr@gmail.com
[github]
  user = thejspr
[color]
  ui = true
[mergetool]
  keepBackup = false
[push]
  default = tracking
[core]
  excludesfile = /Users/jesper/.gitignore
  editor = mvim -v
[alias]
  aa = add --all
  c = commit -v
  d = diff --color-words
  ms = checkout master
  co = checkout
  b = branch
  l = log
  r = remote
  rc = !git add -A && git rebase --continue
  alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\\\t => \\2/' | sort
[apply]
  whitespace = nowarn
[branch]
  autosetupmerge = true
  autosetuprebase = always
